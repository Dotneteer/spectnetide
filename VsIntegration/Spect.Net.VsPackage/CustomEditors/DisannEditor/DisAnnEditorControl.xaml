<UserControl x:Class="Spect.Net.VsPackage.CustomEditors.DisannEditor.DisAnnEditorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             Background="{DynamicResource BToolBackground}"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\..\PackageStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <TabControl Margin="12,8" Background="{DynamicResource BToolBackground}">
            <TabItem Header="Summary"/>
            <TabItem Header="Labels"/>
            <TabItem Header="Literals"/>
            <TabItem Header="Comments"/>
            <TabItem Header="Memory map"/>
        </TabControl>
    </Grid>
</UserControl>
